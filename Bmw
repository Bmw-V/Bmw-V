local PlayerDropdown = Tab:CreateDropdown({
    Name = "Select Player",
    Options = {},
    CurrentOption = {},
    MultipleOptions = false,
    SpecialType = "Player",
    Flag = "PlayerDropdown",
    Callback = function(Option)
        print("Selected player: " .. Option.Name)
    end
})

-- Refresh the dropdown options to reflect current players
local function refreshPlayerList()
    local playerList = {}
    for _, player in ipairs(game.Players:GetPlayers()) do
        table.insert(playerList, player.Name)
    end
    PlayerDropdown:Refresh(playerList, playerList[1])
end

-- Initial population of the dropdown
refreshPlayerList()

-- Update the dropdown options when players join or leave
game.Players.PlayerAdded:Connect(function()
    refreshPlayerList()
end)

game.Players.PlayerRemoving:Connect(function()
    refreshPlayerList()
end)

local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()
local Players = game:GetService("Players")
local lp = Players.LocalPlayer

local function findClosestMatch(input)
    input = input:lower()
    local bestMatch, bestScore = nil, 0
    for _, plr in ipairs(Players:GetPlayers()) do
        local name = plr.DisplayName:lower()
        local score = #input
        if name:sub(1, #input) == input and score > bestScore then
            bestMatch, bestScore = plr, score
        end
    end
    return bestMatch
end

local chosenName = ""

local tab = win:CreateTab("Secret (beta)", 4483362458)

tab:CreateInput({
    Name = "Player Display Name",
    PlaceholderText = "Type a name",
    Callback = function(text)
        chosenName = text
    end
})

tab:CreateButton({
    Name = "Teleport to Player",
    Callback = function()
        local target = findClosestMatch(chosenName)
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            lp.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
        end
    end
})
