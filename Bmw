local Section = Tab:CreateSection("Teleport still in beta",false) -- The 2nd argument is to tell if its only a Title and doesnt contain element

local Players = game:GetService("Players")
local lp = Players.LocalPlayer

local function findClosestMatch(input)
    if not input or input == "" then return nil end
    input = input:lower()
    local bestMatch, bestScore = nil, 0
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= lp then -- Skip local player
            local name = plr.Name:lower() -- Using Name instead of DisplayName for more reliable matching
            local displayName = plr.DisplayName:lower()
            local score = #input
            if (name:sub(1, #input) == input or displayName:sub(1, #input) == input) and score > bestScore then
                bestMatch, bestScore = plr, score
            end
        end
    end
    return bestMatch
end

local chosenName = ""

local selectedPlayer = nil
local playerInput = TeleportTab:CreateInput({
    Name = "Player Name/Display Name",
    PlaceholderText = "Type player name...",
    RemoveTextAfterFocusLost = false,
    Callback = function(text)
        if text == "" then 
            selectedPlayer = nil
            return 
        end
        
        local matchedPlayer = findBestMatch(text)
        if matchedPlayer then
            selectedPlayer = matchedPlayer
            Rayfield:Notify({
                Title = "Player Found",
                Content = "Matched: "..matchedPlayer.Name.." (@@"..matchedPlayer.DisplayName..")",
                Duration = 3,
            })
        else
            Rayfield:Notify({
                Title = "No Match",
                Content = "No player found matching: "..text,
                Duration = 3,
            })
            selectedPlayer = nil
        end
    end,
})

-- Teleport to player button
TeleportTab:CreateButton({
    Name = "Teleport To Player",
    Callback = function()
        if not selectedPlayer then
            Rayfield:Notify({
                Title = "Error",
                Content = "No player selected!",
                Duration = 3,
            })
            return
        end
        
        if not lp.Character or not lp.Character:FindFirstChild("HumanoidRootPart") then
            Rayfield:Notify({
                Title = "Error",
                Content = "Your character isn't loaded!",
                Duration = 3,
            })
            return
        end
        
        if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
            lp.Character.HumanoidRootPart.CFrame = selectedPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 3, 0)
            Rayfield:Notify({
                Title = "Success",
                Content = "Teleported to "..selectedPlayer.Name,
                Duration = 3,
            })
        else
            Rayfield:Notify({
                Title = "Error",
                Content = "Target player's character isn't loaded!",
                Duration = 3,
            })
        end
    end,
})

-- Player list section
TeleportTab:CreateSection("Online Players")

-- Function to update player list
local function updatePlayerList()
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= lp then
            TeleportTab:CreateButton({
                Name = player.Name.." (@@"..player.DisplayName..")",
                Callback = function()
                    selectedPlayer = player
                    playerInput:Set(player.Name)
                    Rayfield:Notify({
                        Title = "Player Selected",
                        Content = "Selected: "..player.Name.." (@@"..player.DisplayName..")",
                        Duration = 3,
                    })
                end,
            })
        end
    end
end

-- Initial player list
updatePlayerList()

-- Update when players join/leave
Players.PlayerAdded:Connect(updatePlayerList)
Players.PlayerRemoved:Connect(updatePlayerList)

